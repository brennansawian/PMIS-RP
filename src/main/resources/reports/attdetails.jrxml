	<queryString>
		<![CDATA[SELECT DISTINCT to_char(TT.programdate,'dd-MM-yyyy') AS programdate,
COALESCE(PA.attendance,'-') AS attendance,
A.applicationcode,A.usercode AS pusercode,U.username AS pusername,
P.programname, P.programid, ph.phaseno, ph.phasedescription ,
to_char(C.startdate,'dd-MM-yyyy') AS startdate,to_char(C.enddate,'dd-MM-yyyy') As enddate,
C.enddate-C.startdate+1 As noofdays,


 array_to_string
                   (
                   ARRAY(
                   SELECT V.venuename FROM nerie.mt_programvenues P
                              INNER JOIN nerie.m_venues V ON V.venuecode=P.venuecode
                   WHERE P.phaseid=C.phaseid
                   ), ', '
                   ) AS venues,
                   array_to_string
                   (
                   ARRAY(
                   SELECT U.username FROM nerie.mt_program_members P
                              INNER JOIN nerie.mt_userlogin U ON U.usercode=P.usercode
                   WHERE P.phaseid=C.phaseid
                   ), ', '
                   ) AS coursecoordinators ,
                                      array_to_string
                   (
                   ARRAY(
                   SELECT T1.subject FROM nerie.t_programtimetable T1
                    WHERE T1.programdate=TT.programdate AND T1.subject!='BREAK'
                   ), ', '
                   ) AS subjects,
                   array_to_string
                   (
                   ARRAY(
                   SELECT DISTINCT T1.rpname FROM nerie.mt_resourcepersons T1
                   INNER JOIN nerie.mt_programttresourceperson U ON U.rpslno=T1.rpslno
                    WHERE U.programtimetablecode=TT.programtimetablecode
                   ), ', '
                   ) AS rpname


 FROM nerie.m_phases ph
 INNER JOIN nerie.t_programtimetable TT ON ph.phaseid=TT.phaseid
 INNER JOIN nerie.t_applications A ON A.phaseid=ph.phaseid
 INNER JOIN nerie.t_participantattendance PA ON PA.programtimetablecode=TT.programtimetablecode
 AND PA.applicationcode=A.applicationcode AND PA.phaseid=A.phaseid
 INNER JOIN nerie.mt_programdetails C on C.phaseid=TT.phaseid
 INNER JOIN nerie.mt_programttresourceperson RP ON TT.programtimetablecode=RP.programtimetablecode
 INNER JOIN nerie.mt_resourcepersons R ON R.rpslno=RP.rpslno
 INNER JOIN nerie.mt_userlogin U ON A.usercode=U.usercode
 INNER JOIN nerie.m_programs P ON P.programcode=ph.programcode
 WHERE TT.phaseid=$P{phaseid}
 ORDER BY A.applicationcode,programdate]]>
