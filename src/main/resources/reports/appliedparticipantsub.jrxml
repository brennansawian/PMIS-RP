	<parameter name="coursecode" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT C.coursename,courseid,startdate,enddate,OF.officename || ',' ||  OF.officeaddress || '-' ||  OF.officepincode AS office,
array_to_string
(
ARRAY(
SELECT UL.username || ',' || D.designationname || ',' || O.officeshortname FROM mt_coursecoordinators CC
INNER JOIN mt_userlogin UL ON CC.usercode=UL.usercode
INNER JOIN m_designations D ON D.designationcode=UL.designationcode
INNER JOIN m_offices O ON O.officecode=UL.officecode
WHERE C.coursecode=CC.coursecode ORDER BY UL.usercode
), ','
) AS coursecoordinator ,
array_to_string
(
ARRAY(
SELECT V.venuename  FROM mt_venuecourses VC
INNER JOIN m_venues V ON VC.venuecode=V.venuecode
WHERE C.coursecode=VC.coursecode ORDER BY V.venuename
), ','
) AS venues
FROM m_courses C
INNER JOIN m_offices OF ON C.officecode=OF.officecode
WHERE C.coursecode=$P{coursecode}]]>
	</queryString>
