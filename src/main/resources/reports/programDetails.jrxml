	<subDataset name="phaseDetails">
		<parameter name="tb_programcode" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT p.phaseno, d.startdate, d.enddate, p.phasedescription,d.closed,d.closingreport,d.finalized,string_agg(distinct(l.username), ',') AS coordinators
FROM nerie.mt_programdetails d, nerie.m_phases p, nerie.mt_program_members m, nerie.mt_userlogin l
WHERE d.programcode = p.programcode AND d.phaseid = p.phaseid AND m.phaseid = p.phaseid AND m.programcode = d.programcode AND l.usercode = m.usercode AND d.programcode=$P{tb_programcode}
GROUP BY  p.phaseno, d.startdate, d.enddate, p.phasedescription,d.closed, d.phaseid, d.closingreport, d.finalized]]>
		</queryString>
		<field name="phaseno" class="java.lang.String"/>
		<field name="startdate" class="java.sql.Date"/>
		<field name="enddate" class="java.sql.Date"/>
		<field name="phasedescription" class="java.lang.String"/>
		<field name="closed" class="java.lang.String"/>
		<field name="closingreport" class="java.lang.String"/>
		<field name="finalized" class="java.lang.String"/>
		<field name="coordinators" class="java.lang.String"/>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
