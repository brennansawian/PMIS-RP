	<parameter name="dynamicpath" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT o.officecode,o.officename,o.officeshortname,o.officeaddress,d.districtname,s.statename,
o.officepincode,c.programid || ': ' || c.programname || '(' || TO_CHAr(pd.startdate,'dd-MM-yyyy') || ' to ' || TO_CHAr(pd.enddate,'dd-MM-yyyy') || ')' || ' [' || cat.coursecategoryname || ' ]'
AS program,
ph.phaseno || ': ' || ph.phasedescription AS phase_details,
u.username AS coursecoordinator
From nerie.m_offices o
inner join nerie.m_districts d on o.officedistrictcode=d.districtcode
inner join nerie.m_states s on d.statecode=s.statecode
left outer join nerie.m_programs c on o.officecode=c.officecode
left outer join nerie.mt_programdetails pd on c.programcode=pd.programcode
left outer join nerie.mt_program_members cc on pd.phaseid=cc.phaseid AND c.programcode=cc.programcode
left outer join nerie.mt_userlogin u on cc.usercode=u.usercode
left outer join nerie.m_coursecategories cat on cat.coursecategorycode=c.coursecodecategory
left outer join nerie.m_phases ph on ph.phaseid=pd.phaseid
WHERE (TO_CHAR(pd.startdate,'yyyy-MM')>=$P{finyearstart} AND TO_CHAR(pd.startdate,'yyyy-MM')<=$P{finyearend}) AND pd.finalized='Y' and o.officecode=$P{officecode}
order by  o.officecode,o.officename,o.officeshortname,o.officeaddress,d.districtname,s.statename,o.officepincode,program,phase_details,u.username]]>
	</queryString>
