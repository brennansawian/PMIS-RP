	<queryString>
		<![CDATA[SELECT a.startdate,a.enddate, a.finalized, b.phaseno, b.phasedescription, c.programid, c.programname,
d.officename, d.officeaddress,d.officepincode, e.statename, f.districtname

FROM nerie.mt_programdetails a, nerie.m_phases b, nerie.m_programs c, nerie.m_offices d, nerie.m_states e,
nerie.m_districts f

WHERE a.phaseid=b.phaseid AND a.programcode=c.programcode AND d.officecode = c.officecode
AND d.officedistrictcode=f.districtcode AND e.statecode = d.statecode
AND a.phaseid=$P{phaseid} AND a.closed = 'N']]>
