	<property name="ireport.y" value="0"/>
	<parameter name="coursecode" class="java.lang.Short"/>
	<parameter name="sectioncode" class="java.lang.Short"/>
	<queryString>
		<![CDATA[SELECT C.coursecode,C.coursename,to_char(C.startdate,'dd-MM-yyyy') AS startdate,to_char(C.enddate,'dd-MM-yyyy') As enddate,C.enddate-C.startdate+1 As noofdays, C.ttfinalized,
 COALESCE(TT.shortcoursetimetablecode,'') AS shortcoursetimetablecode,
   TT.slno,
   COALESCE(TT.coursecode,-1) AS coursecodett,
   TT.coursedate,
   TT.courseday ,
   to_char(starttime,'HH12') || ':' || to_char(starttime,'MI') || ' '|| to_char(starttime,'AM') AS starttime ,
   to_char(endtime,'HH12') || ':' || to_char(endtime,'MI') || ' '|| to_char(endtime,'AM') AS endtime ,
   TT.subject ,
   TT.rpslno , R.rpname,
   TT.usercode ,
   TT.entrydate, COALESCE(V.venuename,'') AS venuename,V.venuecode,
   VR.roomcode,COALESCE(VR.roomname) AS roomname,S.sectioncode,S.sectionname
 FROM m_courses C
 LEFT OUTER JOIN t_shortcoursetimetable TT ON C.coursecode=TT.coursecode
 LEFT OUTER JOIN mt_resourcepersons R ON R.rpslno=TT.rpslno
 LEFT OUTER JOIN m_sections S ON TT.sectioncode=S.sectioncode
 LEFT OUTER JOIN mt_venuerooms VR ON VR.roomcode=TT.roomcode
 LEFT OUTER JOIN m_venues V ON V.venuecode=VR.venuecode
 WHERE C.coursecode=$P{coursecode} AND (0=$P{sectioncode} OR TT.sectioncode=$P{sectioncode})
 ORDER BY S.sectioncode,TT.courseday,TT.starttime]]>
	</queryString>
