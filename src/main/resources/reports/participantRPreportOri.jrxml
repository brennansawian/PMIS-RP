string_agg(distinct u.username||','||d.designationname||','||o.officeshortname,',') as coordinator,string_agg(distinct v.venuename,',') as venuname
from nerie.mt_programdetails pd
inner join nerie.m_programs p on p.programcode=pd.programcode
inner join nerie.m_offices o on p.officecode=o.officecode
inner join nerie.mt_program_members cc on pd.phaseid=cc.phaseid
inner join nerie.mt_userlogin u on u.usercode=cc.usercode and o.officecode=u.officecode
inner join nerie.m_designations d on d.designationcode=u.designationcode
inner join nerie.mt_programvenues vc on vc.phaseid=pd.phaseid
inner join nerie.m_venues v on v.venuecode=vc.venuecode
inner join nerie.m_phases ph on ph.phaseid=pd.phaseid
where pd.phaseid='10'
group by p.programname,p.programid,pd.startdate,pd.enddate,o.officename,o.officeaddress,o.officepincode, p.programcode,ph.phaseno,ph.phasedescription]]>
