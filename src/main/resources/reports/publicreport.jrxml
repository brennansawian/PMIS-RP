	<queryString>
		<![CDATA[select DISTINCT  cc.coursecategorycode,cc.coursecategoryname,p.programcode,p.programname,p.programid,p.programdescription,ph.phaseno,ph.phasedescription,
                   TO_CHAR(pd.startdate,'dd/MM/yyyy') AS startdate,TO_CHAR(pd.enddate,'dd/MM/yyyy') AS enddate,p.officecode,pd.finalized,ph.phaseid,p.programid,TO_CHAR(pd.lastdate,'dd/MM/yyyy') AS lastdate,p.closed,f.officename,TO_CHAR(pd.courseclosedate,'dd/MM/yyyy') AS courseclosedate ,pd.enddate-pd.startdate + 1 || ' days ' AS duration

                   FROM nerie.mt_programdetails pd
                   INNER JOIN nerie.m_programs p ON pd.programcode=p.programcode
                   INNER JOIN nerie.m_coursecategories cc ON cc.coursecategorycode=p.coursecodecategory
                   INNER JOIN nerie.m_offices f ON p.officecode=f.officecode
                   INNER JOIN nerie.m_phases ph ON ph.phaseid=pd.phaseid
WHERE  pd.finalized='Y'
AND TO_CHAR(pd.startdate,'yyyy-MM-dd')<=TO_CHAR(now(),'yyyy-MM-dd') AND TO_CHAR(pd.lastdate,'yyyy-MM-dd')>=TO_CHAR(now(),'yyyy-MM-dd') AND pd.phaseid=$P{phaseid}]]>
