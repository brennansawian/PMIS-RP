	<queryString>
		<![CDATA[--ongoing
select DISTINCT    cc.coursecategorycode,cc.coursecategoryname,p.programcode,p.programname,p.programid,p.programdescription,ph.phaseno,ph.phasedescription,
TO_CHAR(pd.startdate,'dd/MM/yyyy') AS startdate,TO_CHAR(pd.enddate,'dd/MM/yyyy') AS enddate,p.officecode,pd.finalized,ph.phaseid,p.programid,TO_CHAR(pd.lastdate,'dd/MM/yyyy') AS lastdate,p.closed,
f.officename,f.officeshortname,f.officeaddress,d.districtname,s.statename,f.officepincode,
	TO_CHAR(pd.courseclosedate,'dd/MM/yyyy') AS programclosedate,
                   array_to_string
                   (
                   ARRAY(
                   SELECT V.venuename FROM nerie.mt_programvenues P
                              INNER JOIN nerie.m_venues V ON V.venuecode=P.venuecode
                   WHERE P.phaseid=pd.phaseid
                   ), ','
                   ) AS venues,
                   array_to_string
                   (
                   ARRAY(
                   SELECT  U.username || ' (Mob: ' || U.usermobile || ')' FROM nerie.mt_program_members P
                              INNER JOIN nerie.mt_userlogin U ON U.usercode=P.usercode
                   WHERE P.phaseid=pd.phaseid
                   ), E'\n'
                   ) AS coursecoordinators,
		   array_to_string
                   (
                   ARRAY(
                   SELECT  P.rpname || ', ' || rpd.designationname|| '(' || P.rpspecialization || ')' FROM 			nerie.mt_resourcepersons P
                               INNER JOIN nerie.mt_resourcepersoncoursemap U ON U.rpslno=P.rpslno
                              INNER JOIN nerie.m_designations rpd ON rpd.designationcode=P.designationcode
                   WHERE U.phaseid=pd.phaseid
                   ), E'\n'
                   ) AS resource_person, pd.enddate-pd.startdate + 1 || ' days ' AS duration,
                   (SELECT COUNT(DISTINCT applicationcode) FROM nerie.t_applications sp WHERE pd.phaseid=sp.phaseid) As noofparticipants,
                   COALESCE((SELECT SUM(CASE WHEN a.attendance='P' THEN 1 ELSE 0 END)/count(a.programtimetablecode) FROM nerie.t_programtimetable tt INNER JOIN nerie.t_participantattendance a ON a.programtimetablecode=tt.programtimetablecode AND tt.phaseid=pd.phaseid) || '','Not taken') AS avgattd
                   FROM nerie.mt_programdetails pd
                   INNER JOIN nerie.m_programs p ON pd.programcode=p.programcode
                   INNER JOIN nerie.m_coursecategories cc ON cc.coursecategorycode=p.coursecodecategory
                   INNER JOIN nerie.m_offices f ON p.officecode=f.officecode
                   INNER JOIN nerie.m_phases ph ON ph.phaseid=pd.phaseid
		   INNER JOIN nerie.m_districts d on f.officedistrictcode=d.districtcode
		   INNER JOIN nerie.m_states s on d.statecode=s.statecode

WHERE  ((TO_CHAR(pd.startdate,'yyyy-MM')>=$P{finyearstart} AND TO_CHAR(pd.startdate,'yyyy-MM')<=$P{finyearend}) OR ($P{financialyear}='0')) AND pd.finalized='Y' AND
f.officecode=$P{officecode} and
TO_CHAR(pd.startdate,'yyyy-MM-dd')<=TO_CHAR(now(),'yyyy-MM-dd') AND TO_CHAR(pd.enddate,'yyyy-MM-dd')>=TO_CHAR(now(),'yyyy-MM-dd')]]>
	</queryString>
